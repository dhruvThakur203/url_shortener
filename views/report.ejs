<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/style.css"/>
    <title>Report</title>
</head>
<body>

    <%- include('header') %>

    <section class="Report-section">
        <h1>Student report card</h1> 
        <ul>
            <% student.forEach((stud) => { %>
                <li>
                    <p>Name: <%= stud.name %></p>
                    <p>Grade: <%= stud.grade %></p>
                    <p>Favorite Subject: <%= stud.favSubject %></p>
                </li>
            <% }) %>
        </ul>
    </section>
</body>
</html>
